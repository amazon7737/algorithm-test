N = int(input())

A_n = list(map(int, input().split()))
B_n = []
C_n = []
N_2 = N

for i in range(0, N-1):
    if A_n[i] < A_n[i+1]:
        B_n.append(A_n[i+1])
        
    elif A_n[i] > A_n[i+1]:
        N_2 = N_2 -1
    else:
        C_n = -1
        
for _ in range(0, len(B_n)):
    C_n = B_n.pop()

print(int (C_n[N_2])
