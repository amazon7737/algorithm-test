#Q1

def function(n):
    a = "수"
    b = "박"
    c = ""
    for i in range(1, n+1):
        if i %2==0:
            c = c+b
        if i %2!=0:
            c = c+a
        
    return c

        
    
function(5)

#2
n, m = map(int, input().split())
for i in range(1, m+1):
    print("*"*n)
    
    
    
